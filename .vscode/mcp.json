{
  "servers": {
    "ESLint": {
      "type": "stdio",
      "command": "pnpm",
      "args": [
        "dlx",
        "@eslint/mcp@latest"
      ]
    },
    "playwright": {
      "type": "stdio",
      "command": "pnpm",
      "args": [
        "dlx",
        "@playwright/mcp@latest"
      ]
    },
    "github": {
      "type": "http",
      "url": "https://api.githubcopilot.com/mcp/"
    },
    "io.github.ChromeDevTools/chrome-devtools-mcp": {
      "command": "pnpm",
      "args": [
        "dlx",
        "chrome-devtools-mcp@latest",
        "--no-usage-statistics"
      ],
      "env": {},
      "type": "stdio"
    }
  },
  "inputs": []
}
