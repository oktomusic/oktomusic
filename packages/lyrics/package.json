{
  "name": "@oktomusic/lyrics",
  "version": "0.0.0",
  "description": "A starter for creating a TypeScript package.",
  "type": "module",
  "license": "AGPL-3.0-only",
  "homepage": "https://github.com/oktomusic/oktomusic#readme",
  "bugs": {
    "url": "https://github.com/oktomusic/oktomusic/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oktomusic/oktomusic.git"
  },
  "author": "AFCMS <afcm.contact@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && npm publish",
    "lint": "pnpm -w exec eslint -c eslint.config.ts packages/lyrics",
    "lint:fix": "pnpm -w exec eslint -c eslint.config.ts --fix packages/lyrics"
  },
  "devDependencies": {
    "@types/node": "catalog:default",
    "bumpp": "^10.3.2",
    "tsdown": "catalog:default",
    "typescript": "catalog:default"
  },
  "dependencies": {
    "fast-xml-parser": "^5.3.3",
    "zod": "^4.3.4"
  }
}
