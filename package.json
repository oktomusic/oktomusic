{
  "name": "@oktomusic/monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "commitlint": "commitlint --edit",
    "format": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "test:backend": "vitest run --project=@oktomusic/backend",
    "test:backend:watch": "vitest --project=@oktomusic/backend",
    "test:backend:cov": "vitest run --project=@oktomusic/backend --coverage",
    "test:backend:e2e": "vitest run --project=@oktomusic/backend:e2e",
    "test:frontend": "vitest run --project=@oktomusic/frontend",
    "test:frontend:watch": "vitest --project=@oktomusic/frontend",
    "test:frontend:cov": "vitest run --project=@oktomusic/frontend --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "keywords": [],
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@commitlint/types": "^20.2.0",
    "@eslint/js": "^9.39.2",
    "@types/node": "catalog:default",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-lingui": "^0.11.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^17.0.0",
    "graphql-config": "^5.1.5",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.51.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "wrangler": "^4.56.0"
  }
